{
  "name": "node-popup",
  "version": "0.1.2",
  "description": "",
  "main": "cjs.js",
  "scripts": {
    "run-test": "node test/index.js | tap-nyan",
    "precommit-message": "echo 'Pre-commit...' && exit 0",
    "build": "rollup --config",
    "add-post-build-artifacts": "git add ./esm.mjs && git add ./cjs.js",
    "tslint": "tslint",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "lint-fix": "tslint -c tslint.json src/**/*.ts --fix",
    "lint-only-changed": "git diff --staged --name-only HEAD | grep -E '\\.ts$' | xargs tslint",
    "lint-fix-only-changed": "sh lint-changed-files.sh",
    "add-post-lint-fix": "git diff --staged --name-only HEAD | grep -E '\\.ts$' | xargs git add"
  },
  "author": "John Henry",
  "repository": {
    "type": "git",
    "url": "https://github.com/johnhenry/node-popup.git"
  },
  "license": "MIT",
  "dependencies": {
    "carlo": "0.9.43",
    "typescript": "3.3.3333"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@babel/preset-typescript": "7.3.3",
    "pre-commit": "1.2.2",
    "rollup": "1.4.0",
    "rollup-plugin-babel": "4.3.2",
    "tap-nyan": "1.1.0",
    "tape": "4.10.1",
    "tslint": "5.13.1"
  },
  "pre-commit": [
    "precommit-message",
    "lint-fix-only-changed",
    "add-post-lint-fix",
    "build",
    "add-post-build-artifacts"
  ]
}
